{
  "name": "aroosi-mobile",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild:aab": "node scripts/prepare-android-keystore.js",
    "build:aab": "cd android && ./gradlew bundleRelease -x test",
    "publish:internal": "node scripts/publish-internal.js",
    "share:internal": "node scripts/publish-internal.js --share",
    "publish:internal:dry": "node scripts/publish-internal.js --dry-run",
    "postbuild:aab": "npm run aab:where && npm run aab:sha",
    "build:aab:clean": "cd android && ./gradlew clean bundleRelease -x test",
    "aab:where": "echo \"AAB -> android/app/build/outputs/bundle/release/app-release.aab\"",
    "aab:sha": "shasum -a 256 \"android/app/build/outputs/bundle/release/app-release.aab\"",
    "android:devbuild": "eas build --profile development --platform android && npx expo start --android",
    "ios:devbuild": "eas build --profile development --platform ios && npx expo start --ios",
    "ios:devdevice": "eas build --profile development-device --platform ios",
    "ios:devdevice:run": "eas build --profile development-device --platform ios && npx expo start",
    "build": "expo export",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "build:all": "eas build --platform all",
    "build:staging": "eas build --profile staging",
    "build:production": "eas build --profile production",
    "release:ios": "eas build --platform ios --profile production --non-interactive --auto-submit",
    "release:ios:manual": "eas build --platform ios --profile production --non-interactive && eas submit --platform ios --profile production --non-interactive",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "submit:all": "eas submit --platform all",
    "submit:staging": "eas submit --profile staging",
    "submit:production": "eas submit --profile production",
    "update:staging": "eas update --branch staging",
    "update:production": "eas update --branch production",
    "typecheck": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__/(?!integration)",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:e2e": "jest --testPathPattern=__tests__/e2e",
    "test:e2e:staging": "jest --testPathPattern=__tests__/e2e --env=staging",
    "test:e2e:production": "jest --testPathPattern=__tests__/e2e --env=production",
    "test:final-validation": "jest --testPathPattern=finalValidation",
    "test:performance": "jest --testPathPattern=performance",
    "test:accessibility": "jest --testPathPattern=accessibility",
    "test:security": "jest --testPathPattern=security",
    "test:messaging": "node __tests__/messaging/runTests.js",
    "test:messaging:core": "node __tests__/messaging/runTests.js --core",
    "test:messaging:integration": "node __tests__/messaging/runTests.js --integration",
    "test:messaging:sync": "node __tests__/messaging/runTests.js --sync",
    "test:messaging:performance": "node __tests__/messaging/runTests.js --performance",
    "test:messaging:watch": "node __tests__/messaging/runTests.js --watch",
    "test:messaging:coverage": "node __tests__/messaging/runTests.js --coverage",
    "security:scan": "npm audit && npm run test:security",
    "security:audit": "npm audit --audit-level=high",
    "security:fix": "npm audit fix",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "deps:security": "npm audit fix",
    "ci": "npm run lint && npm run type-check && npm run test",
    "ci:full": "npm run ci && npm run test:integration && npm run test:e2e",
    "precommit": "npm run lint && npm run type-check && npm run test:unit",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@expo-google-fonts/bodoni-moda": "^0.4.1",
    "@expo-google-fonts/nunito-sans": "^0.4.1",
    "@expo-google-fonts/playfair-display": "^0.4.1",
    "@expo/ngrok": "^4.1.3",
    "@expo/vector-icons": "^14.1.0",
    "@hookform/resolvers": "^3.9.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-cookies/cookies": "^6.2.1",
    "@react-native-google-signin/google-signin": "^15.0.0",
    "@react-native-picker/picker": "^2.9.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/stack": "^7.4.2",
    "@tanstack/react-query": "^5.64.2",
    "axios": "^1.7.9",
    "eventemitter3": "^5.0.1",
    "expo": "53.0.22",
    "expo-application": "~6.1.5",
    "expo-audio": "~0.4.9",
    "expo-auth-session": "~6.2.1",
    "expo-blur": "^14.1.5",
    "expo-calendar": "~14.1.4",
    "expo-constants": "~17.1.7",
    "expo-contacts": "~14.2.5",
    "expo-dev-client": "5.2.4",
    "expo-device": "~7.1.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image-manipulator": "~13.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "^14.1.5",
    "expo-linking": "~7.1.7",
    "expo-location": "~18.1.6",
    "expo-media-library": "~17.1.7",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.5",
    "expo-secure-store": "~14.2.4",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.17",
    "expo-web-browser": "^14.2.0",
    "firebase": "^10.12.4",
    "jest-expo": "~53.0.10",
    "jwt-decode": "^4.0.0",
    "onesignal-expo-plugin": "^2.0.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.0",
    "react-native": "0.79.5",
    "react-native-draggable-flatlist": "^4.0.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-iap": "^12.16.3",
    "react-native-image-picker": "^7.1.2",
    "react-native-onesignal": "^5.2.12",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "^4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-toast-message": "^2.3.3",
    "rn-emoji-keyboard": "^1.7.0",
    "zod": "^3.24.1",
    "expo-apple-authentication": "~7.2.4",
    "expo-crypto": "~14.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.34.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.10",
    "react-test-renderer": "19.0.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "enabled": true,
        "exclude": [
          "@react-native-cookies/cookies",
          "react-native-iap"
        ],
        "listUnknownPackages": false
      },
      "appConfigFieldsNotSyncedCheck": {
        "enabled": false
      }
    }
  }
}
