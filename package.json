{
  "name": "aroosi-mobile",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "start:tunnel": "expo start --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "android:local": "eas build --profile staging --platform android --local",
    "android:local:aab": "eas build --profile production --platform android --local",
    "ios:local": "eas build --profile development-device --platform ios --local",
    "ios:local:staging": "eas build --profile staging --platform ios --local",
    "ios:run:precompiled": "RCT_USE_PREBUILT_RNCORE=1 RCT_USE_RN_DEP=1 npx pod-install && expo run:ios",
    "web": "expo start --web",
    "prebuild:aab": "node scripts/prepare-android-keystore.js",
    "build:aab": "cd android && ./gradlew bundleRelease -x test",
    "publish:internal": "node scripts/publish-internal.js",
    "share:internal": "node scripts/publish-internal.js --share",
    "publish:internal:dry": "node scripts/publish-internal.js --dry-run",
    "postbuild:aab": "npm run aab:where && npm run aab:sha",
    "build:aab:clean": "cd android && ./gradlew clean bundleRelease -x test",
    "android:apk:release": "cd android && ./gradlew assembleRelease -x test",
    "android:apk:debug": "cd android && ./gradlew assembleDebug -x test",
    "aab:where": "echo \"AAB -> android/app/build/outputs/bundle/release/app-release.aab\"",
    "aab:sha": "shasum -a 256 \"android/app/build/outputs/bundle/release/app-release.aab\"",
    "predev:check": "node scripts/check-google-services.js",
    "android:devbuild": "npm run predev:check && eas build --profile development --platform android && npx expo start --tunnel",
    "ios:devbuild": "npm run predev:check && eas build --profile development-device --platform ios && npx expo start --tunnel",
    "ios:devdevice": "eas build --profile development-device --platform ios",
    "ios:devdevice:run": "eas build --profile development-device --platform ios && npx expo start",
    "build": "expo export",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "build:all": "eas build --platform all",
    "build:staging": "eas build --profile staging",
    "build:production": "eas build --profile production",
    "release:ios": "eas build --platform ios --profile production --non-interactive --auto-submit",
    "release:ios:manual": "eas build --platform ios --profile production --non-interactive && eas submit --platform ios --profile production --non-interactive",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "submit:all": "eas submit --platform all",
    "submit:staging": "eas submit --profile staging",
    "submit:production": "eas submit --profile production",
    "update:staging": "eas update --branch staging",
    "update:production": "eas update --branch production",
    "typecheck": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern='__tests__/(?!integration)'",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:e2e": "jest --testPathPattern=__tests__/e2e",
    "test:e2e:staging": "jest --testPathPattern=__tests__/e2e --env=staging",
    "test:e2e:production": "jest --testPathPattern=__tests__/e2e --env=production",
    "test:final-validation": "jest --testPathPattern=finalValidation",
    "test:performance": "jest --testPathPattern=performance",
    "test:accessibility": "jest --testPathPattern=accessibility",
    "test:security": "jest --testPathPattern=security",
    "test:messaging": "node __tests__/messaging/runTests.js",
    "test:messaging:core": "node __tests__/messaging/runTests.js --core",
    "test:messaging:integration": "node __tests__/messaging/runTests.js --integration",
    "test:messaging:sync": "node __tests__/messaging/runTests.js --sync",
    "test:messaging:performance": "node __tests__/messaging/runTests.js --performance",
    "test:messaging:watch": "node __tests__/messaging/runTests.js --watch",
    "test:messaging:coverage": "node __tests__/messaging/runTests.js --coverage",
    "security:scan": "npm audit && npm run test:security",
    "security:audit": "npm audit --audit-level=high",
    "security:fix": "npm audit fix",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "deps:security": "npm audit fix",
    "ci": "npm run lint && npm run type-check && npm run test",
    "ci:full": "npm run ci && npm run test:integration && npm run test:e2e",
    "precommit": "npm run lint && npm run type-check && npm run test:unit",
    "prepare": "husky install || true",
    "eas-build-post-install": "node scripts/eas-post-install.js",
    "bump:patch": "node scripts/bump-version.js patch",
    "bump:minor": "node scripts/bump-version.js minor",
    "bump:major": "node scripts/bump-version.js major",
    "tag": "node scripts/post-submit-version.js --push",
    "eas:build:ios:staging": "eas build --platform ios --profile staging",
    "eas:build:android:staging": "eas build --platform android --profile staging",
    "eas:build:ios:prod": "eas build --platform ios --profile production",
    "eas:build:android:prod": "eas build --platform android --profile production",
    "eas:submit:ios:staging": "eas submit -p ios --profile staging",
    "eas:submit:android:staging": "eas submit -p android --profile staging",
    "eas:submit:ios:prod": "eas submit -p ios --profile production",
    "eas:submit:android:prod": "eas submit -p android --profile production",
    "release:staging:ios": "npm run bump:patch && npm run eas:build:ios:staging && npm run eas:submit:ios:staging && npm run tag",
    "release:staging:android": "npm run bump:patch && npm run eas:build:android:staging && npm run eas:submit:android:staging && npm run tag",
    "release:prod:ios": "npm run bump:patch && npm run eas:build:ios:prod && npm run eas:submit:ios:prod && npm run tag",
    "release:prod:android": "npm run bump:patch && npm run eas:build:android:prod && npm run eas:submit:android:prod && npm run tag"
  },
  "dependencies": {
    "@expo-google-fonts/bodoni-moda": "^0.4.1",
    "@expo-google-fonts/nunito-sans": "^0.4.1",
    "@expo-google-fonts/playfair-display": "^0.4.1",
    "@expo/ngrok": "^4.1.3",
    "@expo/vector-icons": "^15.0.2",
    "@hookform/resolvers": "^3.9.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-cookies/cookies": "^6.2.1",
    "@react-native-google-signin/google-signin": "^15.0.0",
    "@react-native-picker/picker": "^2.9.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/stack": "^7.4.2",
    "@sentry/react-native": "^7.0.1",
    "@tanstack/react-query": "^5.64.2",
    "axios": "^1.7.9",
    "eventemitter3": "^5.0.1",
    "expo": "^54.0.0",
    "expo-apple-authentication": "~8.0.6",
    "expo-application": "~7.0.6",
    "expo-audio": "~1.0.10",
    "expo-blur": "~15.0.6",
    "expo-calendar": "~15.0.6",
    "expo-constants": "~18.0.8",
    "expo-contacts": "~15.0.8",
    "expo-crypto": "~15.0.6",
    "expo-dev-client": "~6.0.11",
    "expo-device": "~8.0.6",
    "expo-font": "~14.0.7",
    "expo-haptics": "~15.0.6",
    "expo-image-manipulator": "~14.0.7",
    "expo-image-picker": "~17.0.7",
    "expo-linear-gradient": "~15.0.6",
    "expo-linking": "~8.0.7",
    "expo-location": "~19.0.6",
    "expo-media-library": "~18.1.1",
    "expo-notifications": "~0.32.10",
    "expo-router": "~6.0.0",
    "expo-secure-store": "~15.0.6",
    "expo-status-bar": "~3.0.7",
    "expo-updates": "~29.0.9",
    "firebase": "^12.2.1",
    "jwt-decode": "^4.0.0",
    "onesignal-expo-plugin": "^2.0.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.54.0",
    "react-native": "0.81.4",
    "react-native-deck-swiper": "^2.0.19",
    "react-native-draggable-flatlist": "^4.0.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-iap": "^14.2.2",
    "react-native-image-picker": "^7.1.2",
    "react-native-onesignal": "^5.2.12",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-toast-message": "^2.3.3",
    "react-native-worklets": "0.5.1",
    "rn-emoji-keyboard": "^1.7.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@react-native-community/cli": "^20.0.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^9.34.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.10",
    "react-test-renderer": "19.1.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  },
  "overrides": {
    "expo-dev-menu": "7.0.10",
    "expo-dev-launcher": "6.0.10"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "enabled": true,
        "exclude": [
          "@react-native-cookies/cookies",
          "react-native-iap"
        ],
        "listUnknownPackages": false
      },
      "appConfigFieldsNotSyncedCheck": {
        "enabled": false
      }
    }
  }
}
